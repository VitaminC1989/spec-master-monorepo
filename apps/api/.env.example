# 数据库配置
DATABASE_URL="postgresql://admin:123@localhost:5432/specmaster?schema=public"
PORT=3000

# Sealos 对象存储配置
# 请复制此文件为 .env.local 并填入真实的密钥信息
# 或运行脚本：bash scripts/setup-object-storage-env.sh

OSS_ENDPOINT=https://objectstorageapi.hzh.sealos.run
OSS_REGION=us-east-1
OSS_BUCKET=your-bucket-name
OSS_ACCESS_KEY=your_access_key_here
OSS_SECRET_KEY=your_secret_key_here
OSS_PUBLIC_BASE_URL=https://objectstorageapi.hzh.sealos.run/your-bucket-name
OSS_PRESIGN_EXPIRES_IN=900

# CORS 配置
CORS_ORIGINS=http://localhost:5173
